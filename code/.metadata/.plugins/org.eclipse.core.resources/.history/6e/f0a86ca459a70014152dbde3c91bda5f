#include "WPILib.h"
#include <Victor.h>
#include <RobotDrive.h>
#include <Joystick.h>

class Robot: public IterativeRobot
{
private:
	LiveWindow *lw;
	Victor v1, v2;
	RobotDrive drive;
	Joystick stick;

	void RobotInit()
	{
		lw = LiveWindow::GetInstance();
		v1(0);
		v2(1);
		drive(v1, v2);
		stick(0);
	}

	void AutonomousInit()
	{

	}

	void AutonomousPeriodic()
	{
		drive.ArcadeDrive(stick);
	}

	void TeleopInit()
	{

	}

	void TeleopPeriodic()
	{

	}

	void TestPeriodic()
	{
		lw->Run();
	}
};

START_ROBOT_CLASS(Robot);
