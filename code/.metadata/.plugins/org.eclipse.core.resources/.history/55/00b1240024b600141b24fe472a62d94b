#include <Subsystems/ToteHolderSubsystem.h>
#include <RobotMap.h>

ToteHolderSubsystem::ToteHolderSubsystem():
Subsystem("ToteHolderSubsystem")
{
	leftExtensionSwitch  = new DigitalInput(ToteHolderLeftExtensionSwitchPort);
	leftRetractionSwitch = new DigitalInput(ToteHolderLeftRetractionSwitchPort);
	rightExtensionSwitch = new DigitalInput(ToteHolderRightExtensionSwitchPort);
	rightRetractionSwitch = new DigitalInput(ToteHolderRightRetractionSwitchPort);
	leftMotor   = new Jaguar(ToteHolderLeftJaguarPort);
	rightMotor  = new Jaguar(ToteHolderRightJaguarPort);
}

void ToteHolderSubsystem::setDirection()
{

}
