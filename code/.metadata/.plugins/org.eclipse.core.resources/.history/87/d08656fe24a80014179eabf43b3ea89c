#ifndef SRC_COMMANDS_DRIVEROBOT_H_
#define SRC_COMMANDS_DRIVEROBOT_H_

#include <CommandBase.h>

namespace robot {

class DriveRobot: public CommandBase {
private:

public:
	DriveRobot();

	void Initialize();
	void Execute();
	void IsFinished();
	void End();
	void IsInterrupted();

	void SetDrive(Joystick* controller);
};

} /* namespace robot */

#endif /* SRC_COMMANDS_DRIVEROBOT_H_ */
